{
    "name": "colbygatte/colby-wiki",
    "require": {
        "colbygatte/cg-utilities": "dev-master",
        "monolog/monolog": "^1.23",
        "erusev/parsedown": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2",
        "symfony/var-dumper": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Wiki\\": "src/Wiki"
        },
        "files": ["utilities/utilities.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/utilities.php"]
    },
    "authors": [
        {
            "name": "colbygatte",
            "email": "colbygatte@gmail.com"
        }
    ]
}
